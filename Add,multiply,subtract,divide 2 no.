#include <stdio.h>
int main() {
    int a, b;
    int sum, diff, product;
    float div;

    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    sum = a + b;
    diff = a - b;
    product = a * b;
    div = (float)a / b;   // typecasting to get decimal result

    printf("Sum = %d\n", sum);
    printf("Difference = %d\n", diff);
    printf("Product = %d\n", product);
    printf("Division = %.2f\n", div);

    return 0;
}
