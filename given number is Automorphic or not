#include <stdio.h>
#include <math.h>
int main() {
    int n, sq, temp, digits=0, pow10;
    printf("Enter number: ");
    scanf("%d",&n);
    sq = n*n;
    temp = n;
    while(temp>0) {
        digits++;
        temp/=10;
    }
    pow10 = pow(10, digits);
    if(sq % pow10 == n)
        printf("Automorphic Number");
    else
        printf("Not Automorphic Number");
    return 0;
}
